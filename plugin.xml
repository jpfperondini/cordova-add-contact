<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-add-contact" version="0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>cordova-add-contact</name>
    <js-module name="cordova-add-contact" src="www/cordova-add-contact.js"><clobbers target="cordova.plugins.cordova-add-contact"/></js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="cordova-add-contact"><param name="android-package" value="cordova.add.contact"/></feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file><source-file src="src/android/cordovaAddContact.java" target-dir="src/cordova-add-contact/cordova-add-contact"/></platform>
</plugin>
